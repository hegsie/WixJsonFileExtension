{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["store"],
  "properties": {
    "store": {
      "type": "object",
      "properties": {
        "book": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["category", "author", "title", "price"],
            "properties": {
              "category": {
                "type": "string"
              },
              "author": {
                "type": "string"
              },
              "title": {
                "type": "string"
              },
              "isbn": {
                "type": "string"
              },
              "price": {
                "type": "number"
              }
            }
          }
        },
        "bicycle": {
          "type": "object",
          "properties": {
            "color": {
              "type": "string"
            },
            "price": {
              "type": "number"
            }
          }
        }
      }
    },
    "Serilog": {
      "type": "object"
    },
    "expensive": {
      "type": "number"
    }
  }
}
