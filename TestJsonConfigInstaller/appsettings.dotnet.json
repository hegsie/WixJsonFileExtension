{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=MyApp;Integrated Security=true;",
    "ReportingConnection": "Server=reporting-server;Database=Reports;Integrated Security=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "AppName": "MyApplication",
    "Version": "1.0.0",
    "DataPath": "C:\\ProgramData\\MyApp\\Data",
    "TempPath": "C:\\ProgramData\\MyApp\\Temp"
  },
  "ServiceSettings": {
    "ServiceName": "MyAppService",
    "DisplayName": "My Application Service",
    "Description": "This service runs My Application",
    "StartType": "Automatic"
  },
  "ApiSettings": {
    "BaseUrl": "https://api.example.com",
    "Timeout": 30,
    "RetryCount": 3
  },
  "FeatureFlags": {
    "EnableNewFeature": false,
    "EnableBetaFeatures": false
  }
}
